#include <iostream>
#include <vector>
#include <string>
#include <cmath>
#include <cstdlib>
using namespace std;
void myStrCat(char* dest, const char* src) {
    while (*dest != '\0') dest++;
    while (*src != '\0') {
        *dest = *src;
        dest++;
        src++;
    }
    *dest = '\0';
}

int main3() {
    char str1[50] = "Hello, ";
    char str2[] = "World!";
    myStrCat(str1, str2);
    cout << "Результат конкатенации: " << str1 << endl;
    return 0;
}
